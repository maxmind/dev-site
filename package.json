  "engines": {
    "node": ">=12.16.0",
    "yarn": "^1.17.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "ts-jest"
  },
  "license": "MIT",
  "lint-staged": {
    ".*rc.js": [
      "yarn fix:configs"
    ],
    "**/*.{json}": [
      "yarn fix:json"
    ],
    "content/**/*.{mdx}": [
      "yarn fix:content"
    ],
    "**/*.{js,ts,tsx}": [
      "yarn fix:scripts"
    ],
    "src/**/*.{[s]css}": [
      "yarn fix:styles"
    ]
  },
  "name": "dev-site",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/maxmind/dev-site.git"
  },
  "resolutions": {
    "node-fetch": "^2.6.1",
    "node-forge": "^0.10.0",
    "yargs-parser": "^18.1.2"
  },
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "deploy": "node ./firebase.js && firebase deploy --only hosting:staging",
    "deploy:ci": "node ./firebase.js && firebase deploy --only hosting:production --token \"$FIREBASE_TOKEN\"",
    "develop": "gatsby clean && gatsby develop",
    "fix": "run-p fix:*",
    "fix:configs": "yarn lint:configs --fix",
    "fix:content": "prettier --write **/*.mdx --embedded-language-formatting off --loglevel silent && remark . --output --ext mdx --quiet",
    "fix:json": "yarn lint:json --fix",
    "fix:scripts": "yarn lint:scripts --fix",
    "fix:styles": "yarn lint:styles --fix",
    "lint": "run-p lint:*",
    "lint:configs": "eslint .*rc.js --no-ignore",
    "lint:content": "remark . --ext mdx --quiet",
    "lint:json": "eslint . --ext json",
    "lint:scripts": "eslint . --ext js,ts,tsx",
    "lint:styles": "stylelint ./src/**/*.{css,scss,sass}",
    "serve": "node ./firebase.js && firebase serve --only hosting:local",
    "start": "yarn run develop",
    "test": "jest"
  },
  "version": "0.1.0"
}
