{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@site/*": [
        "./src/*"
      ],
      "@theme/*": [
        "./node_modules/gatsby-theme-maxmind/*"
      ],
    },
    "typeRoots": [
      "./../../packages/gatsby-theme-maxmind/src/types",
      "./../../node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules",
  ],
  "extends": "../../tsconfig.json",
  "include": [
    "./content/**/*",
    "./src/**/*"
  ]
}
