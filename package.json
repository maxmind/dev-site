    "**/*.{json}": [
      "yarn fix:json"
    ],
    "content/**/*.{mdx}": [
      "yarn fix:content"
    ],
    "**/*.{js,ts,tsx}": [
      "yarn fix:scripts"
    ],
    "src/**/*.{[s]css}": [
      "yarn fix:styles"
    ]
  },
  "name": "dev-site",
  "private": true,
  "resolutions": {
    "@mdx-js/mdx": "2.0.0-next.8",
    "@mdx-js/react": "2.0.0-next.8",
    "@mdx-js/runtime": "2.0.0-next.8",
    "@mdx-js/util": "2.0.0-next.8",
    "remark-mdx": "2.0.0-next.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxmind/dev-site.git"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:gatsby": "gatsby clean && gatsby build",
    "build:functions": "cd functions && yarn build",
    "deploy": "firebase deploy",
    "develop": "gatsby clean && GATSBY_URL=http://localhost:8000 gatsby develop",
    "fix": "run-p fix:*",
    "fix:content": "prettier --write **/*.mdx --embedded-language-formatting off --loglevel silent && remark . --output --ext mdx --quiet",
    "fix:json": "yarn lint:json --fix",
    "fix:scripts": "yarn lint:scripts --fix",
    "fix:styles": "yarn lint:styles --fix",
    "lint": "run-p lint:*",
    "lint:configs": "eslint .*rc.js --no-ignore",
    "lint:content": "remark . --ext mdx --quiet",
    "lint:json": "eslint . --ext json",
    "lint:scripts": "eslint . --ext js,ts,tsx",
    "lint:styles": "stylelint ./src/**/*.{css,scss,sass}",
    "postinstall": "yarn --cwd functions install",
    "preview": "firebase hosting:channel:deploy $(git rev-parse --abbrev-ref HEAD)",
    "serve": "firebase emulators:start --only functions,hosting",
    "start": "yarn run develop",
    "test": "run-s \"test:* --silent\"",
    "test:a11y": "jest --config .jestrc.a11y.js --no-cache --maxWorkers=4",
    "test:unit": "jest --config .jestrc.js",
    "test:regressions": "jest --config .jestrc.regressions.js"
  },
  "version": "0.1.0"
}
