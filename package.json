  "license": "MIT",
  "lint-staged": {
    "**/*.json": [
      "npx eslint --fix --config '.eslintrc.js'"
    ],
    "content/**/*.mdx": [
      "npx prettier --write --embedded-language-formatting off --config './content/.prettierrc.js'"
    ],
    "**/*.{js,ts,tsx}": [
      "npx eslint --fix --ignore-pattern 'functions/**/*.js' --config '.eslintrc.js'"
    ],
    "src/**/*.[s]css": [
      "npx stylelint --fix --config '.stylelintrc.js'"
    ]
  },
  "name": "dev-site",
  "private": true,
  "resolutions": {
    "@mdx-js/mdx": "2.0.0-next.8",
    "@mdx-js/react": "2.1.3",
    "@mdx-js/runtime": "2.0.0-next.9",
    "@mdx-js/util": "2.0.0-next.8",
    "remark-mdx": "2.0.0-next.8",
    "remark-mdxjs": "2.0.0-next.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxmind/dev-site.git"
  },
  "scripts": {
    "build": "run-p build:* && npm run sitemap-query-string",
    "build:gatsby": "gatsby clean && gatsby build",
    "build:firebase-config": "node firebase/index.js",
    "build:functions": "cd functions && npm run build",
    "deploy": "firebase deploy",
    "develop": "gatsby clean && GATSBY_URL=http://localhost:8000 gatsby develop",
    "fix": "run-p fix:*",
    "fix:content": "prettier --write '**/*.mdx' --embedded-language-formatting off",
    "fix:json": "npm run lint:json --fix",
    "fix:scripts": "npm run lint:scripts --fix",
    "fix:styles": "npm run lint:styles --fix",
    "lint": "run-p lint:*",
    "lint:configs": "eslint .*rc.js --no-ignore",
    "lint:content": "prettier --check '**/*.mdx' --embedded-language-formatting off",
    "lint:json": "eslint . --ext json",
    "lint:scripts": "tsc && eslint . --ext js,ts,tsx --ignore-pattern 'functions/**/*.js'",
    "lint:styles": "stylelint ./src/**/*.{css,scss,sass}",
    "preview": "firebase hosting:channel:deploy $(git rev-parse --abbrev-ref HEAD)",
    "serve": "firebase emulators:start --only functions,hosting",
    "sitemap-query-string": "node ./bin/sitemap-query-string.js",
    "start": "npm run develop",
    "test": "run-s \"test:* --silent\"",
    "test:unit": "jest --config .jestrc.js",
    "test:e2e": "jest --config .jestrc.e2e.js",
    "test:broken-links": "npm run test:e2e --verbose -t 'website has no broken links'",
    "test:redirects": "npm run build:firebase-config && npm run test:e2e --verbose -t 'Static file redirects'"
  },
  "version": "0.1.0",
  "workspaces": {
    "packages": [
      "functions"
    ]
  }
}
