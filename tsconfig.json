{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "ES2019"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": false,
    "resolveJsonModule": true,
    "strict": true,
    "target": "ES2019",
  },
  "exclude": [
    "node_modules",
  ],
}
