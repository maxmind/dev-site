    "remark-lint-no-empty-url": "^2.0.0",
    "remark-lint-write-good": "^1.2.0",
    "remark-preset-github": "^3.0.0",
    "remark-preset-lint-markdown-style-guide": "^3.0.0",
    "remark-preset-lint-recommended": "^4.0.0",
    "stylelint": "^13.3.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-order": "^4.0.0",
    "ts-jest": "^26.4.0"
  },
  "engines": {
    "node": ">=10.13.0",
    "yarn": "^1.17.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/gatsby-ts",
      "<rootDir>/src",
      "<rootDir>/utils"
    ]
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.{json}": [
      "lint:json"
    ],
    "content/**/*.{mdx}": [
      "lint:content"
    ],
    "**/*.{js,ts,tsx}": [
      "lint:scripts"
    ],
    "src/**/*.{[s]css}": [
      "lint:styles"
    ]
  },
  "name": "dev-site",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/maxmind/dev-site.git"
  },
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "deploy": "node ./firebase-json.js && firebase deploy --only hosting:staging",
    "deploy:ci": "node ./firebase-json.js && firebase deploy --only hosting:production --token \"$FIREBASE_TOKEN\"",
    "develop": "gatsby clean && gatsby develop",
    "lint": "run-p lint:*",
    "lint:configs": "eslint .*rc.js --no-ignore",
    "lint:content": "remark . --ext mdx",
    "lint:json": "eslint . --ext json",
    "lint:scripts": "eslint . --ext js,ts,tsx",
    "lint:styles": "stylelint ./src/**/*.{css,scss,sass}",
    "serve": "node ./firebase-json.js && firebase serve --only hosting:local",
    "start": "yarn run develop",
    "test": "jest"
  },
  "version": "0.1.0"
}
