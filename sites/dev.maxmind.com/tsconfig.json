{
  "compilerOptions": {
    "baseUrl": ".",
    "lib": [
      "dom",
      "ES2019"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "paths": {
      "@site/*": [
        "./src/*"
      ],
      "@theme/*": [
        "../../packages/gatsby-theme-maxmind/src/*"
      ]
    },
    "typeRoots": [
      "../../packages/gatsby-theme-maxmind/src/types",
      "../../node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules",
  ],
  "extends": "../../tsconfig.json",
  "include": [
    "./content/**/*",
    "./src/**/*"
  ]
}
