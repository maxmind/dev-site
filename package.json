{
  "author": "MaxMind",
  "bugs": {
    "url": "https://github.com/maxmind/dev-site/issues"
  },
  "description": "Developer website for MaxMind",
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.3.1",
    "cspell": "^9.2.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "stylelint": "^16.24.0",
    "stylelint-config-recommended": "^17.0.0",
    "prettier-plugin-go-template": "^0.0.15",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-no-unsupported-browser-features": "^8.0.4",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=9"
  },
  "license": "MIT",
  "name": "dev-site",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/maxmind/dev-site.git"
  },
  "scripts": {
    "fix": "run-p fix:*",
    "fix:scripts": "npm run lint:scripts --fix",
    "fix:styles": "npm run lint:styles --fix",
    "lint": "run-p lint:*",
    "lint:cspell": "cspell '**/*.md' --no-summary --no-progress --color",
    "lint:scripts": "tsc && npx eslint .  --max-warnings=0",
    "lint:styles": "stylelint ./assets/scss/*.{css,scss,sass}",
    "lint:shortcode-parity": "npx tsx ./bin/check-shortcode-parity.ts",
    "prettier:ts": "prettier --write './assets/js/**'",
    "format:rawhtml": "npx tsc format-rawhtml.ts --outDir dist && node dist/format-rawhtml.js"
  },
  "version": "1.0.0"
}
